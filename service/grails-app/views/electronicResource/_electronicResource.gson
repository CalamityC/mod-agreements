import groovy.transform.*
import org.olf.kb.ElectronicResource

model {
  ElectronicResource electronicResource
}
json g.render(electronicResource, [expand: ['plat']]) {

  // Add the custom summary option
  if ( electronicResource.type == 'title' ) {
    resourceCount 1
  }
  else {
    resourceCount 'a number'
  }
}



